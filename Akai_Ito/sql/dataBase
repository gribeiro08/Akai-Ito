tem que fazer o sql ;-;

o nome do database deve ser akaiito
nele tera duas tabelas: user e user_adm

na tabela user tera 7 colunas
1 id : int (A.I)
2 username : varchar(50, NULL)
3 data_nasc : date
4 story_progress : int(50)
5 usertype : varchar(50, NULL)
6 full_name : varchar(50, NULL)
7 password : varchar(50, NULL)

na tabela user_adm tera 4 colunas
1 id : int (A.I)
2 username : varchar(50)
3 usertype : varchar(50)
4 password : varchar(50)

CREATE TABLE `akaiito`.`user` (`id` INT NOT NULL , `username` VARCHAR(50) NOT NULL , `data_nasc` DATE NOT NULL , `story_progress` INT(50) NOT NULL , `usertype` VARCHAR(50) NOT NULL , `full_name` VARCHAR(50) NOT NULL , `password` VARCHAR(50) NOT NULL ) ENGINE = InnoDB;
ALTER TABLE `user` CHANGE `id` `id` INT(11) NOT NULL AUTO_INCREMENT, add PRIMARY KEY (`id`), CHANGE `username` `username` VARCHAR(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL, CHANGE `data_nasc` `data_nasc` DATE NULL, CHANGE `story_progress` `story_progress` INT(50) NULL, CHANGE `usertype` `usertype` VARCHAR(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL, CHANGE `full_name` `full_name` VARCHAR(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL, CHANGE `password` `password` VARCHAR(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_general_ci NULL;


CREATE TABLE `akaiito`.`user_adm` (`id` INT NOT NULL AUTO_INCREMENT , `username` VARCHAR(50) NOT NULL , `usertype` VARCHAR(50) NOT NULL , `password` VARCHAR(50) NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;
INSERT INTO `user_adm` (`id`, `username`, `usertype`, `password`) VALUES (NULL, 'admin1', 'admin', '1234');